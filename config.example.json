{
  "host": "127.0.0.1",
  "port": 3000,
  "timeout_seconds": 30,
  "environment": "development",
  "external_apis": {
    "moralis": {
      "base_url": "REPLACE_WITH_PINAX_ENDPOINT",
      "api_key": "REPLACE_WITH_YOUR_MORALIS_API_KEY",
      "timeout_seconds": 30,
      "health_check_timeout_seconds": 5,
      "max_retries": 3,
      "enabled": false
    },
    "pinax": {
      "endpoint": "REPLACE_WITH_PINAX_ENDPOINT",
      "api_user": "REPLACE_WITH_YOUR_PINAX_USERNAME",
      "api_auth": "REPLACE_WITH_YOUR_PINAX_AUTH_TOKEN",
      "db_name": "REPLACE_WITH_DB_NAME",
      "timeout_seconds": 30,
      "health_check_timeout_seconds": 5,
      "max_retries": 3,
      "enabled": false
    }
  },
  "rate_limiting": {
    "enabled": true,
    "requests_per_minute": 60
  },
  "extensions": {}
}
